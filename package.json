{
  "name": "medicalink-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "ldblcks-258",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "format:check": "prettier --check \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/medicalink-microservice/main",
    "start:gateway": "nest start api-gateway --watch",
    "start:accounts": "nest start accounts-service --watch",
    "start:provider": "nest start provider-directory-service --watch",
    "start:booking": "nest start booking-service --watch",
    "start:content": "nest start content-service --watch",
    "start:notification": "nest start notification-service --watch",
    "start:orchestrator": "nest start orchestrator-service --watch",
    "dev": "concurrently \"pnpm run start:accounts\" \"pnpm run start:provider\" \"pnpm run start:booking\" \"pnpm run start:content\" \"pnpm run start:notification\" \"pnpm run start:orchestrator\" \"pnpm run start:gateway\"",
    "dev-docker:up": "cd ./development && docker-compose up -d",
    "dev-docker:down": "cd ./development && docker-compose down",
    "dev-docker:logs": "cd ./development && docker-compose logs -f",
    "setup": "powershell -ExecutionPolicy Bypass -File setup.ps1",
    "prisma:generate": "cd apps/accounts-service && npx prisma generate && cd ../.. && cd apps/provider-directory-service && npx prisma generate && cd ../.. && cd apps/booking-service && npx prisma generate && cd ../.. && cd apps/content-service && npx prisma generate && cd ../.. && cd apps/notification-service && npx prisma generate && cd ../..",
    "prisma:generate:accounts": "cd apps/accounts-service && npx prisma generate && cd ../..",
    "prisma:generate:provider": "cd apps/provider-directory-service && npx prisma generate && cd ../..",
    "prisma:generate:booking": "cd apps/booking-service && npx prisma generate && cd ../..",
    "prisma:generate:content": "cd apps/content-service && npx prisma generate && cd ../..",
    "prisma:generate:notification": "cd apps/notification-service && npx prisma generate && cd ../..",
    "prisma:push": "cd apps/accounts-service && npx prisma db push && cd ../.. && cd apps/provider-directory-service && npx prisma db push && cd ../.. && cd apps/booking-service && npx prisma db push && cd ../.. && cd apps/content-service && npx prisma db push && cd ../.. && cd apps/notification-service && npx prisma db push && cd ../..",
    "build:accounts": "nest build accounts-service",
    "build:provider": "nest build provider-directory-service",
    "build:booking": "nest build booking-service",
    "build:content": "nest build content-service",
    "build:notification": "nest build notification-service",
    "build:orchestrator": "nest build orchestrator-service",
    "build:gateway": "nest build api-gateway",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/medicalink-microservice/test/jest-e2e.json",
    "script": "node ./scripts/run-script.js"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.16.3",
    "@types/bcrypt": "^6.0.0",
    "@types/morgan": "^1.9.10",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.9",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "bullmq": "^5.58.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.7.0",
    "dotenv": "^17.2.2",
    "ioredis": "^5.7.0",
    "morgan": "^1.10.1",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.4.2",
    "prisma": "^6.16.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/contracts(|/.*)$": "<rootDir>/libs/contracts/src/$1"
    }
  }
}
